    "browser-sync": "^2.18.2",
    "chai": "^3.5.0",
    "chokidar": "^1.6.1",
    "css-loader": "^0.26.0",
    "editorconfig-tools": "^0.1.1",
    "enzyme": "^2.6.0",
    "eslint": "^3.13.1",
    "eslint-config-airbnb": "^13.0.0",
